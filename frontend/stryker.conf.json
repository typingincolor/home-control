{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": ["html", "clear-text", "progress"],
  "testRunner": "vitest",
  "vitest": {
    "configFile": "vitest.config.js"
  },
  "coverageAnalysis": "perTest",
  "mutate": [
    "src/utils/**/*.js",
    "!src/utils/**/*.test.js"
  ],
  "thresholds": {
    "high": 80,
    "low": 60,
    "break": 50
  }
}
